{
  "name": "contaoblackforest/contao-subpages-navigation",
  "description": "Provides a Navigation of all Child-Pages at next level from current Page",
  "keywords": ["contao", "navigation", "subpages"],
  "type": "contao-module",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Sven Baumann",
      "email": "baumann.sv@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Dominik Tomasi",
      "email": "dominik.tomasi@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/ContaoBlackForest/contao-subpages-navigation/issues",
    "source": "https://github.com/ContaoBlackForest/contao-subpages-navigation"
  },
  "require": {
    "php": ">=5.3",
    "contao/core": ">=3.2-dev,<4-dev",
    "contao-community-alliance/composer-plugin": "~2.0",
    "contao-community-alliance/meta-palettes": "~1.9"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "ContaoBlackForest\\SubPagesNavigation\\": "src/"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "module": "system/modules/subpages-navigation"
      }
    },
    "branch-alias": {
      "dev-develop": "1.0.x-dev"
    }
  }
}
